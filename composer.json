{
	"name": "purplespider/file-listing",
	"description": "Basic Download Page functionality, based on an assets directory",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/purplespider/silverstripe-file-listing",
	"keywords": [
		"silverstripe",
		"files",
		"download"
	],
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "James Cocker",
		"email": "ssmodules@pswd.biz"
	}],
	"support": {
		"issues": "https://github.com/purplespider/silverstripe-file-listing/issues"
	},
	"require": {
		"silverstripe/framework": "^4 || ^5",
		"silverstripe/cms": "^4 || ^5"
	},
	"replace": {
		"silverstripe/file-listing": "*"
	},
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "PurpleSpider\\FileListing\\": "src/"
        }
    }
}
